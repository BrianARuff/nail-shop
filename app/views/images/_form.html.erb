<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_with(model: @images) do |f| %>

      <div class="form-group">
        <%= f.label :background_image %>
        <%= f.file_field :background_image %>
      </div>

      <div class="form-group">
        <%= f.label :shop_image1 %>
        <%= f.file_field :shop_image1 %>
      </div>

      <div class="form-group">
        <%= f.label :shop_image2 %>
        <%= f.file_field :shop_image2 %>
      </div>

      <div class="form-group">
        <%= f.label :shop_image3 %>
        <%= f.file_field :shop_image3 %>
      </div>

      <div class="form-group">
        <%= f.label :shop_image4 %>
        <%= f.file_field :shop_image4 %>
      </div>

      <div class="form-group">
        <%= f.label :shop_image5 %>
        <%= f.file_field :shop_image5 %>
      </div>

      <%= f.hidden_field :user_id, value: current_user.id %>

      <div class="actions">
        <%= f.submit 'Submit Pictures', class: 'btn btn-primary btn-block'
        %> <br>
      </div>
    <% end %>
    <%= link_to 'Home', root_path, class: 'btn btn-default btn-block' %>
    <%= link_to 'Images', images_path, class: 'btn btn-default btn-block' %>
  </div>
</div>